  <script type='text/javascript'>
    /*global performance */
    var googletag = window.googletag || {};
    googletag.cmd = googletag.cmd || [];
    var pbjs = pbjs || {};
    pbjs.que = pbjs.que || [];

    var PREBID_TIMEOUT = 500;
    var PREBID_DEBUG = document.location.search.indexOf('pbjs_debug') !== -1;

    function prebid_log() {
        if (PREBID_DEBUG) {
            // http://stackoverflow.com/questions/5538972/console-log-apply-not-working-in-ie9#comment8444540_5539378
            Function.prototype.apply.call(console.log, console, arguments);
        }
    }

    SF.Ads.initAdServer = function(calledBy) {
        if (pbjs.initAdserverSet) {
            return;
        }
            (function() {
                SF.Ads.initAdServerCalledBy = calledBy;
                prebid_log('load main gpt script');
                if (PREBID_DEBUG) {
                    performance.mark('load-gpt-start');
                }

                var gads = document.createElement('script');
                gads.async = true;
                gads.type = 'text/javascript';
                var useSSL = 'https:' === document.location.protocol;
                gads.src = (useSSL ? 'https:' : 'http:') +
                    '//www.googletagservices.com/tag/js/gpt.js';

                
                SF.Ads.gpt_loaded = false;
                function gpt_callback(){
                    SF.Ads.gpt_loaded = true;
                }
                function gpt_onerror(){
                    SF.Ads.gpt_blocked = true;
                }
                gads.onload=gpt_callback;
                gads.onerror=gpt_onerror;

                var node = document.getElementsByTagName('script')[0];
                node.parentNode.insertBefore(gads, node);
            })();pbjs.initAdserverSet = true;
    };
    SF.Ads.prebidWinners = SF.Ads.prebidWinners || {};

    SF.Ads.initPrebid = function(){

        pbjs.que.push(function() {
            prebid_log('pbjs set price granularity');
            pbjs.setPriceGranularity("high");
        });

        pbjs.que.push(function() {

            if (PREBID_DEBUG) {
                pbjs.onEvent('bidRequested', function (info) {
                    for (var i = 0; i < info.bids.length; i++) {
                        var bid = info.bids[i];
                        performance.mark(bid.bidder + '-' + bid.placementCode + '-start');
                    }
                });
                pbjs.onEvent('bidResponse', function (bid) {
                    var name = bid.bidder + '-' + bid.adUnitCode;
                    performance.mark(name + '-end');
                    performance.measure(name, name+'-start', name+'-end');
                });
            }
            pbjs.onEvent('bidWon', function (bid) {
                var adposition = SF.Ads.Helpers.getPathFromContainerID(bid.adUnitCode);
                SF.Ads.prebidWinners[adposition] = bid;
                prebid_log('bidWon:', adposition, bid.adId, bid);
            });
            pbjs.bidderSettings = {
                
                sovrn: {
                    bidCpmAdjustment: function(bidCpm) {
                            return bidCpm * 1.11305;
                    }
                },
                
                aardvark: {
                    bidCpmAdjustment: function(bidCpm) {
                            return bidCpm * 0.011187;
                    }
                },
                
                aol: {
                    bidCpmAdjustment: function(bidCpm) {
                            return bidCpm * 0.89044;
                    }
                },
                
                brealtime: {
                    bidCpmAdjustment: function(bidCpm) {
                            return bidCpm * 0.950895;
                    }
                },
                
                rubicon: {
                    bidCpmAdjustment: function(bidCpm) {
                            return bidCpm * 0.91826625;
                    }
                },
                
                pulsepoint: {
                    bidCpmAdjustment: function(bidCpm) {
                            return bidCpm * 1.09836;
                    }
                },
                
                appnexus: {
                    bidCpmAdjustment: function(bidCpm) {
                            return bidCpm * 1.11305;
                    }
                },
                
                springserve: {
                    bidCpmAdjustment: function(bidCpm) {
                            return bidCpm * 1.13;
                    }
                },
                
            };

            prebid_log('pbjs units pushed');
            var adUnits = [{
